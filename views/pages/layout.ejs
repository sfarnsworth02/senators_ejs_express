<!DOCTYPE html>
<html>
    <head>
        <%- include('../components/head') %>
    </head>
    <body>
        <header>
            <%- include('../components/header') %>
        </header>
        <main>
            <h1>Senators List</h1>
            <!-- <p>Hello</p> -->
            <ul>
                <% senators.forEach(function(senator){ %>
                    <li><a href='/person/<%= senator.person.osid %>'><%= senator.person.sortname %></a></li>
                <% }); %>
            </ul>
        </main>
    </body>
</html>
